<h1>List of all actors</h1>
<hr>

<table border="1">
    <tr>
      <th>ID</th>
      <th>Image</th>
      <th>Name</th>
      <th>Dob</th>
      <th></th>
    </tr>
  <%@director_list.each do |display|%>
    <tr>
      <td><%=display.id%></td>
      <td>
        <img src="<%=display.image%>" style="width:250px">
      </td>
      <td><%=display.name%></td>
      <td><%=display.dob%></td>
      <td>
        <a href="/directors/<%=display.id%>">
          Show details
        </a>
      </td>
    </tr>
  <%end%>
</table>